@{
    ViewData["Title"] = "Prank";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<style>
    .containerNew {
        max-width: 80%;
        width: 100%;
        margin: 0 auto;
    }
</style>

<div class="containerNew">
    <div class="row">
        <div class="col-md-8 text-left">
            <h2>Manage Prank Call From Phone Numeber</h2>
        </div>
        <div class="form-inline col-md-4 div-right">
            <div class="input-group">
                <input type="text" class="form-control" placeholder="Search By Country,Code and Phone Number" id="txtSearch">
                <div class="input-group-append">
                    <button class="btn btn-success" type="submit" onclick="javascript: DataTableInit();"><i class="fa fa-search"></i></button>
                </div>
                <button class="btn bg-facebook border" onclick="javascript: AddEditPrankCallFromPhoneNumber(0,'Add Prank Call From Phone Number');">Add</button>
            </div>
        </div>
            <div class="col-md-12 mt-4  table-responsive" id="dvPrankCallFromPhoneNumeberLst">
                <table class="table table-bordered table-hover" id="dataTable">
                    <thead class="thead-light ">
                        <tr>
                            <th scope="col">Country </th>
                            <th scope="col">Country Code </th>
                            <th scope="col">From Phone Number</th>
                            <th scope="col">IsActive</th>
                            <th scope="col">AddedDate</th>
                            <th scope="col">Action</th>

                        </tr>
                    </thead>

                </table>
            </div>
        </div>
    </div>

@*Add Edit modal popup*@

<div class="modal fade" id="PrankCallFromPhoneNumeberAddEditModal" role="dialog">
    <div class="modal-dialog modal-custom">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="CallFromNumberHeading">Add/Edit Prank Call From Phone Number</h5>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
                <div id="dvAddEditPrankCallFromPhoneNumeber"></div>
            </div>
            <div class="modal-footer">
                <button type="submit" class="btn btn-default btn-primary pull-right" id="btnSavePrankCallFromPhoneNumeber"> Save</button>
                <button type="button" class="btn btn-default btn-default pull-right" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span> Close</button>

            </div>
        </div>
    </div>
</div>
@*<script src="~/js/jquery-ui.min.js"></script>
<script src="~/DataTable/jquery.dataTables.min.js" type="text/javascript"></script>*@

